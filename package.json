{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lark-raycast",
  "title": "Lark",
  "description": "Lark / Feishu integretion for Raycast.",
  "icon": "lark.svg",
  "author": "reekystive",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-tasks",
      "title": "Open Tasks",
      "subtitle": "Lark",
      "description": "Open tasks in Lark.",
      "mode": "no-view",
      "icon": "lark.svg"
    },
    {
      "name": "create-task",
      "title": "Create Task",
      "subtitle": "Lark",
      "description": "Create a task in Lark.",
      "mode": "no-view",
      "icon": "lark.svg"
    },
    {
      "name": "open-calendar",
      "title": "Open Calendar",
      "subtitle": "Lark",
      "description": "Open calendar in Lark.",
      "mode": "no-view",
      "icon": "lark.svg"
    },
    {
      "name": "create-event",
      "title": "Create Event",
      "subtitle": "Lark",
      "description": "Create a calendar event in Lark.",
      "mode": "no-view",
      "icon": "lark.svg"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
